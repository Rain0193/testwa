{
  "name": "testwa",
  "private": true,
  "version": "0.1.0",
  "description": "Automated test Script recording tool for Apps.",
  "author": "canfeit <canfeit@qq.com> (http://testwa.com/)",
  "license": "MIT",
  "keywords": [
    "testwa",
    "appium",
    "selenium",
    "webdriver",
    "automation",
    "testing"
  ],
  "tags": [
    "testwa",
    "appium",
    "selenium",
    "webdriver",
    "automation",
    "testing",
    "javascript",
    "ios",
    "android",
    "firefoxos"
  ],
  "homepage": "https://github.com/canfeit/testwa#readme",
  "scripts": {
    "start": "electron-webpack dev",
    "package": "yarn compile && build --publish always",
    "package-all": "yarn compile && build -mwl",
    "package-linux": "yarn compile && build --linux",
    "package-win": "yarn compile && build --win --x64",
    "compile": "electron-webpack",
    "preinstall": "conda activate py2",
    "prepackage": "standard-version&&git push --follow-tags"
  },
  "build": {
    "productName": "testwa录制工具",
    "appId": "com.testwa.desktop",
    "copyright": "Copyright © 2018 testwa",
    "files": [
      "!**/{LICENSE,LICENCE,license,CHANGELOG,*LICENSE,LICENSE*,AUTHORS,.gitmodules}",
      "!**/*.{png,jpg,gif,map,un~}"
    ],
    "extraResources": [
      "static/apks",
      "static/minicap/libs",
      "static/minicap/jni/minicap-shared/aosp/libs",
      "static/minitouch/libs"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": ["nsis"]
    },
    "linux": {
      "target": ["deb", "AppImage"],
      "category": "Development"
    },
    "directories": {
      "output": "release"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/canfeit/testwa.git"
  },
  "bugs": {
    "url": "https://github.com/canfeit/testwa/issues"
  },
  "dependencies": {
    "adbkit": ">=2.11.0",
    "source-map-support": "^0.5.6"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.0.0-beta.47",
    "@material-ui/core": "^1.0.0",
    "appium-uiautomator2-server": "^1.12.0",
    "electron": "2.0.2",
    "electron-builder": "20.14.7",
    "electron-updater": "^2.21.10",
    "electron-webpack": "2.1.2",
    "minicap": "^0.2.1-0",
    "react": "16.4.0",
    "react-dom": "16.4.0",
    "standard-version": "4.4.0",
    "styletron-engine-atomic": ">=1.0.5",
    "styletron-react": ">=4.2.1",
    "wd": "^1.7.0",
    "webpack": ">=4.8.3"
  }
}
