{
  "name": "testwa",
  "private": true,
  "version": "0.1.4",
  "description": "Automated test Script recording tool for Apps.",
  "author": "canfeit <canfeit@qq.com> (http://testwa.com/)",
  "license": "MIT",
  "keywords": [
    "testwa",
    "appium",
    "selenium",
    "webdriver",
    "automation",
    "testing"
  ],
  "tags": [
    "testwa",
    "appium",
    "selenium",
    "webdriver",
    "automation",
    "testing",
    "javascript",
    "ios",
    "android",
    "firefoxos"
  ],
  "homepage": "https://github.com/canfeit/testwa#readme",
  "scripts": {
    "build": "npm run build:untouched && npm run build:optimized",
    "build:untouched": "asc assembly/module.ts -t module.untouched.wat -b module.untouched.wasm --validate --sourceMap --measure",
    "build:optimized": "asc assembly/module.ts -t module.optimized.wat -b module.optimized.wasm --validate --sourceMap --measure --optimize",
    "start": "electron-webpack dev",
    "package": "yarn compile && build -mwl --publish always",
    "package-all": "yarn compile && build -mwl",
    "package-mac": "yarn compile && build --mac",
    "package-win": "yarn compile && build --win --x64",
    "compile": "electron-webpack",
    "preinstall": "conda activate py2||echo '若安装失败请手动执行：conda activate py2,然后重试'",
    "prepackage": "standard-version&&git push --follow-tags"
  },
  "build": {
    "productName": "testwa录制工具",
    "appId": "com.testwa.desktop",
    "copyright": "Copyright © 2018 testwa",
    "files": [
      "testwa/**/*.js",
      "!**/{LICENSE,LICENCE,license,CHANGELOG,*LICENSE,LICENSE*,AUTHORS,.gitmodules}",
      "!**/*.{png,jpg,gif,map,un~}"
    ],
    "extraResources": [
      "static/js",
      "static/uiautomator2/apks",
      "static/minicap/libs",
      "static/minicap/jni/minicap-shared/aosp/libs",
      "static/minitouch/libs"
    ],
    "directories": {
      "output": "release"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/canfeit/testwa.git"
  },
  "bugs": {
    "url": "https://github.com/canfeit/testwa/issues"
  },
  "dependencies": {
    "adbkit": "^2.11.0",
    "pouchdb-browser": "^6.4.3",
    "source-map-support": "^0.5.6"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.0.0-beta.51",
    "antd": "^3.6.3",
    "appium": "^1.8.1",
    "assemblyscript": "AssemblyScript/assemblyscript",
    "axios": "^0.18.0",
    "electron": "^2.0.2",
    "electron-builder": "^20.15.1",
    "electron-updater": "^2.21.10",
    "electron-webpack": "^2.1.2",
    "highlight.js": "^9.12.0",
    "minicap": "^0.2.1-0",
    "moment": "^2.22.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "request": "^2.87.0",
    "standard-version": "^4.4.0",
    "styletron-engine-atomic": "^1.0.5",
    "styletron-react": "^4.3.0",
    "wd": "^1.9.0",
    "webpack": "^4.12.0",
    "xpath": "^0.0.27"
  },
  "electronWebpack": {
    "renderer": {
      "webpackConfig": "webpack.config.js"
    }
  }
}
